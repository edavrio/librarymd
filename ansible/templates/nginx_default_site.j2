server {
        listen 80 default_server;

        root /www/webapp;
        #root /srv/website;
        index index.php;

        server_name _;

        location / {
            try_files $uri $uri/ =404;
        }

        location ~ [^/]\.php(/|$) {
            include snippets/fastcgi-php.conf;

            fastcgi_pass unix:/var/run/php/php5.6-fpm.sock;
        }
}